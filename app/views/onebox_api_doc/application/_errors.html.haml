%table.ui.celled.structured.errors.table
  %thead
    %tr
      %th Code
      %th Message
      %th Params
  %tbody
    - codes.each do |code|
      %tr
        %td.error__code
          = code._code
        %td.error__msg
          = code._message
        %td.error__params
          = render partial: 'onebox_api_doc/application/params', locals: { params: code._params } unless code._params.blank?
